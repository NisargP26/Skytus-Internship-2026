@model IEnumerable<Assn8.Models.Student>

<h2>Students</h2>
<a href="/Student/Create">Add Student</a>

<table border="1">
<tr>
  <th>Name</th><th>Dept</th><th>Marks</th><th>Actions</th>
</tr>

@foreach (var s in Model)
{
<tr>
  <td>@s.Name</td>
  <td>@s.Department</td>
  <td>@s.Marks</td>
  <td>
    <a href="/Student/Edit/@s.Id">Edit</a> |
    <a href="/Student/Delete/@s.Id">Delete</a>
  </td>
</tr>
}
</table>
