@model List<Assn7.Models.Student>

<h2>Students from Database</h2>

<table border="1" cellpadding="5">
    <tr>
        <th>Name</th>
        <th>Department</th>
        <th>Marks</th>
    </tr>

@foreach (var s in Model)
{
    <tr>
        <td>@s.Name</td>
        <td>@s.Department</td>
        <td>@s.Marks</td>
    </tr>
}
</table>
